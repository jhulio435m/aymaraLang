{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "AymaraLang",
  "scopeName": "source.aymara",
  "patterns": [
    {
      "name": "comment.line.number-sign.aymara",
      "match": "#.*$"
    },
    {
      "name": "string.quoted.double.aymara",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.aymara",
          "match": "\\\\."
        }
      ]
    },
    {
      "name": "constant.numeric.aymara",
      "match": "\\b\\d+(\\.\\d+)?\\b"
    },
    {
      "name": "keyword.control.aymara",
      "match": "\\b(si|sino|mientras|haceña|para|tantachaña|kutiyana|apu|luräwi)\\b"
    },
    {
      "name": "storage.type.aymara",
      "match": "\\b(jach’a|lliphiphi|qillqa|chuymani)\\b"
    },
    {
      "name": "support.function.builtin.aymara",
      "match": "\\b(willt’aña|input|range)\\b"
    }
  ],
  "repository": {}
}
