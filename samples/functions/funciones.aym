// Ejemplos de funciones

// función sin retorno
lurawi saludar(aru nombre) {
    qillqa("Kamisaraki");
    qillqa(nombre);
}

// función con retorno
lurawi sumar(jakhüwi a, jakhüwi b) : jakhüwi {
    kuttaya(a + b);
}

// función recursiva
lurawi fact(jakhüwi n) : jakhüwi {
    suti (n == 0) {
        kuttaya(1);
    }
    kuttaya(n * fact(n - 1));
}

saludar("mundo");
qillqa(sumar(3, 4));
qillqa(fact(5));
tukuya
