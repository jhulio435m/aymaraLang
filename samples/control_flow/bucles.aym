// Ejemplos de bucles

// while
yatiya jakhüwi i = 0;
kunawsati (i < 3) {
    qillqa(i);
    i = i + 1;
}

// for tradicional
sapüru (yatiya jakhüwi j = 0; j < 5; j = j + 1;) {
    qillqa(j);
}

// for con condición interna
sapüru (yatiya jakhüwi k = 0; k < 5; k = k + 1;) {
    suti (k == 2) {
        qillqa("salta dos");
    }
    suti (k == 4) {
        qillqa("llega cuatro");
    }
    qillqa(k);
}
