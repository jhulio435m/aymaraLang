yatiya jakhüwi n = 5;
jisa(n > 3) {
  qillqa("jach'a");
}

yatiya jakhüwi n2 = 2;
jisa(n2 > 3) {
  qillqa("jach'a");
} maysatxa {
  qillqa("jisk'a");
}

yatiya jakhüwi n3 = -1;
jisa(n3 > 0) {
  qillqa("positivu");
} maysatxa {
  jisa(n3 == 0) {
    qillqa("ch'usa");
  } maysatxa {
    qillqa("negativu");
  }
}

yatiya jakhüwi n4 = 10;
jisa(n4 > 0) {
  jisa(n4 % 2 == 0) {
    qillqa("positivu par");
  } maysatxa {
    qillqa("positivu impar");
  }
} maysatxa {
  qillqa("jani positivukiti");
}

yatiya jakhüwi edad = 17;
yatiya chiqa permiso = chiqa;
jisa((edad >= 18) || permiso) {
  qillqa("mantani");
} maysatxa {
  qillqa("jani mantani");
}

yatiya chiqa ok = k'ari;
jisa(!ok) {
  qillqa("jani ok");
}

yatiya jakhüwi n5 = 1;
jisa(n5 == 1) { qillqa("maya"); } maysatxa { qillqa("jani maya"); }

yatiya jakhüwi n6 = 3;
qillqa(n6 % 2 == 0 ? "par" : "impar");

yatiya jakhüwi n7 = 0;
qillqa(n7 > 0 ? "positivu" : (n7 == 0 ? "ch'usa" : "negativu"));

yatiya jakhüwi i = 0;
ukhakamaxa(i < 3) {
  qillqa(i);
  i++;
}

yatiya jakhüwi j = 0;
ukhakamaxa(chiqa) {
  jisa(j == 2) { p'akhiña; }
  qillqa(j);
  j++;
}

taki(yatiya jakhüwi k = 0; k < 3; k++) {
  qillqa(k);
}

taki(yatiya jakhüwi m = 3; m > 0; m--) {
  qillqa(m);
}

taki(yatiya jakhüwi p = 0; p < 5; p++) {
  jisa(p == 2) { sarantaña; }
  qillqa(p);
}

taki(yatiya jakhüwi t = 0; t < 6; t++) {
  jisa(t == 4) { p'akhiña; }
  jisa(t % 2 == 0) {
    qillqa(t % 2 == 0 ? "par" : "impar");
  } maysatxa {
    sarantaña;
  }
}
