{
  "Imprimir": {
    "prefix": "qillqa",
    "body": "qillqa(${1:mensaje});",
    "description": "Imprimir con salto de línea"
  },
  "Leer input": {
    "prefix": "input",
    "body": "yatiya ${1:jakhüwi} ${2:nombre} = input();",
    "description": "Lectura por consola"
  },
  "Suti / Jani": {
    "prefix": "suti",
    "body": [
      "suti (${1:condicion}) {",
      "    ${2:// ...}",
      "} jani {",
      "    ${3:// ...}",
      "}"
    ],
    "description": "Condicional suti/jani"
  },
  "Kunawsati": {
    "prefix": "kunawsati",
    "body": [
      "kunawsati (${1:condicion}) {",
      "    ${2:// ...}",
      "}"
    ],
    "description": "Bucle mientras"
  },
  "Sapüru clásico": {
    "prefix": "sapuru",
    "body": [
      "sapüru (yatiya ${1:jakhüwi} ${2:i} = 0; ${3:i < 10}; ${4:i = i + 1;}) {",
      "    ${4:// ...}",
      "}"
    ],
    "description": "Bucle para clásico"
  },
  "Función": {
    "prefix": "lurawi",
    "body": [
      "lurawi ${1:nombre}(${2:jakhüwi param}) {",
      "    ${3:// ...}",
      "}"
    ],
    "description": "Definición de función"
  },
  "Retorno": {
    "prefix": "kuttaya",
    "body": "kuttaya ${1:valor};",
    "description": "Retorno de función"
  }
}
