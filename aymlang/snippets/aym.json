{
  "Imprimir": {
    "prefix": "qillqa",
    "body": "qillqa(${1:mensaje});",
    "description": "Imprimir con salto de línea"
  },
  "Programa básico": {
    "prefix": "qallta",
    "body": [
      "qallta",
      "    ${1:// ...}",
      "tukuya"
    ],
    "description": "Estructura básica de programa"
  },
  "Jisa / Maysatxa": {
    "prefix": "jisa",
    "body": [
      "jisa (${1:condicion}) {",
        "    ${2:// ...}",
      "} maysatxa {",
      "    ${3:// ...}",
      "}"
    ],
    "description": "Condicional jisa/maysatxa"
  },
  "Ukhakamaxa": {
    "prefix": "ukhakamaxa",
    "body": [
      "ukhakamaxa (${1:condicion}) {",
      "    ${2:// ...}",
      "}"
    ],
    "description": "Bucle mientras"
  },
  "Taki clásico": {
    "prefix": "taki",
    "body": [
      "taki (yatiya ${1:jakhüwi} ${2:i} = 0; ${3:i < 10}; ${4:i++}) {",
      "    ${5:// ...}",
      "}"
    ],
    "description": "Bucle para clásico"
  },
  "Función": {
    "prefix": "lurawi",
    "body": [
      "lurawi ${1:nombre}(${2:jakhüwi param}) {",
      "    ${3:// ...}",
      "}"
    ],
    "description": "Definición de función"
  },
  "Clase": {
    "prefix": "kasta",
    "body": [
      "kasta ${1:Nombre} {",
      "    ${2:// ...}",
      "}"
    ],
    "description": "Declaración de clase"
  },
  "Try / Catch / Finally": {
    "prefix": "yant'aña",
    "body": [
      "yant'aña {",
      "    ${1:// ...}",
      "} katjaña (${2:error}) {",
      "    ${3:// ...}",
      "} tukuyawi {",
      "    ${4:// ...}",
      "}"
    ],
    "description": "Manejo de errores"
  },
  "Retorno": {
    "prefix": "kuttaya",
    "body": "kuttaya ${1:valor};",
    "description": "Retorno de función"
  }
}
