{
  "Imprimir": {
    "prefix": "qillqa",
    "body": "qillqa(${1:mensaje});",
    "description": "Imprimir con salto de línea"
  },
  "Leer con katu": {
    "prefix": "katu",
    "body": "yatiya ${1:aru} ${2:nombre} = katu(\"${3:Mensaje}: \", tuku=\"\");",
    "description": "Lectura por consola con prompt"
  },
  "Jisa / Maysatxa": {
    "prefix": "jisa",
    "body": [
      "jisa (${1:condicion}) {",
      "    ${2:// ...}",
      "} maysatxa {",
      "    ${3:// ...}",
      "}"
    ],
    "description": "Condicional jisa/maysatxa"
  },
  "Ukhakamaxa": {
    "prefix": "ukhakamaxa",
    "body": [
      "ukhakamaxa (${1:condicion}) {",
      "    ${2:// ...}",
      "}"
    ],
    "description": "Bucle mientras"
  },
  "Taki clásico": {
    "prefix": "taki",
    "body": [
      "taki (yatiya ${1:jakhüwi} ${2:i} = 0; ${3:i < 10}; ${4:i++}) {",
      "    ${5:// ...}",
      "}"
    ],
    "description": "Bucle para clásico"
  },
  "Función": {
    "prefix": "lurawi",
    "body": [
      "lurawi ${1:nombre}(${2:jakhüwi param}) {",
      "    ${3:// ...}",
      "}"
    ],
    "description": "Definición de función"
  },
  "Retorno": {
    "prefix": "kuttaya",
    "body": "kuttaya ${1:valor};",
    "description": "Retorno de función"
  }
}
