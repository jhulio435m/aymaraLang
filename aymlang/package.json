{
  "name": "aymlang",
  "displayName": "aymLang",
  "description": "Extensión oficial para el lenguaje de programación AymaraLang con resaltado de sintaxis, snippets y autocompletado",
  "version": "0.0.3",
  "publisher": "JhulioMoran",
  "icon": "icon.png",
  "main": "./src/extension.js",
  "activationEvents": [
    "onLanguage:aym"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jhulio435m/aymaraLang"
  },
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "aym",
      "aliases": ["aymlang", "aym"],
      "extensions": [".aym"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "aym",
      "scopeName": "source.aym",
      "path": "./syntaxes/aym.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "aym",
        "path": "./snippets/aym.json"
      }
    ]
  },
  "license": "LICENSE"
}
